        <servlet-class>com.cultivattfg.bd.RegistroProducto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModificarProducto</servlet-name>
        <servlet-class>com.cultivattfg.bd.ModificarProducto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminarProducto</servlet-name>
        <servlet-class>com.cultivattfg.bd.EliminarProducto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModificarDatosProducto</servlet-name>
        <servlet-class>com.cultivattfg.bd.ModificarDatosProducto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProcesarPedido</servlet-name>
        <servlet-class>com.cultivattfg.bd.ProcesarPedido</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TramitarPedido</servlet-name>
        <servlet-class>com.cultivattfg.bd.TramitarPedido</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CambiarEstadoPedido</servlet-name>
        <servlet-class>com.cultivattfg.bd.CambiarEstadoPedido</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginUsuario</servlet-name>
        <url-pattern>/LoginUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CerrarSesion</servlet-name>
        <url-pattern>/CerrarSesion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistroUsuario</servlet-name>
        <url-pattern>/RegistroUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistroUsuarioProductor</servlet-name>
        <url-pattern>/RegistroUsuarioProductor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModificarDatosUsuario</servlet-name>
        <url-pattern>/ModificarDatosUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModificarDatosProductor</servlet-name>
        <url-pattern>/ModificarDatosProductor</url-pattern>
    </servlet-mapping>
