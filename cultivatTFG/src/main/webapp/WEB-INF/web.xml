        <url-pattern>/ProcesarPedido</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TramitarPedido</servlet-name>
        <url-pattern>/TramitarPedido</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CambiarEstadoPedido</servlet-name>
        <url-pattern>/CambiarEstadoPedido</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
